{
  "name": "hvoya-crm",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format:all": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "NODE_ENV=test nest start --watch",
    "start:test-headless": "NODE_ENV=test nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:ui": "npm run cypress:start --prefix client",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm-extension": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js",
    "migrations:create": "npm run typeorm migration:create -- ./src/modules/database/migrations/$npm_config_name",
    "migrations:generate": "npm run typeorm migration:generate -- ./src/modules/database/migrations/$npm_config_name -d ./typeorm.config.ts",
    "migrations:revert": "npm run typeorm migration:revert -- -d ./typeorm.config.ts",
    "migrations:run": "npm run typeorm migration:run -- -d ./typeorm.config.ts",
    "schema:drop": "npm run typeorm schema:drop -- -d ./typeorm.config.ts",
    "schema:sync": "npm run typeorm schema:sync -- -d ./typeorm.config.ts",
    "schema:log": "npm run typeorm schema:log -- -d ./typeorm.config.ts",
    "db:create": "npm run typeorm-extension db:create -- --initialDatabase $(grep INITIAL_DATABASE ./env/.env | cut -d '=' -f2) -d ./typeorm.config.ts",
    "db:drop": "npm run typeorm-extension db:drop -- --initialDatabase $(grep INITIAL_DATABASE ./env/.env | cut -d '=' -f2) -d ./typeorm.config.ts",
    "db:seed": "npm run typeorm-extension seed -- -d ./typeorm.config.ts",
    "db:reset": "npm run schema:drop && npm run migrations:run && npm run db:seed",
    "db:setup": "npm run db:drop && npm run db:create && npm run migrations:run && npm run db:seed",
    "db:setup-test": "NODE_ENV=test npm run db:setup"
  },
  "pre-push": [
    "test:ui"
  ],
  "dependencies": {
    "@casl/ability": "^6.3.3",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.0.0",
    "@newrelic/native-metrics": "^9.0.0",
    "@newrelic/winston-enricher": "^4.0.1",
    "@types/winston": "^2.4.4",
    "bcrypt": "^5.1.0",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.33.0",
    "compression": "^1.7.4",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^6.7.0",
    "express-xml-bodyparser": "^0.3.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "nest-winston": "^1.9.2",
    "newrelic": "^10.1.2",
    "nocache": "^3.0.4",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "pm2": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.0",
    "winston": "^3.8.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/buffer-to-stream": "^1.0.0",
    "@types/compression": "^1.7.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-xml-bodyparser": "^0.3.2",
    "@types/jest": "28.1.8",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.7",
    "@types/newrelic": "^9.14.0",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "pre-push": "^0.1.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}