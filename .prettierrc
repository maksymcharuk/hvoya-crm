{
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^(?:@nestjs|@casl|@angular)/(.*)$",
    "^(?:@enums|@interfaces|@entities|@dtos|@decorators|@transformers|@shared|@environment|@layout)/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "decorators-legacy"
  ]
}